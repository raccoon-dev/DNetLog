unit DNLog.WinMsg;

interface
  
function WinMsgToStr(const Msg: Cardinal): String;

implementation

uses
  System.SysUtils, Vcl.Controls, System.Generics.Collections;

var
  MsgDict: TDictionary<Cardinal, String>;

function WinMsgToStr(const Msg: Cardinal): String;
begin
  if not MsgDict.TryGetValue(Msg, Result) then
    Result := Format('[Unknown DEC=%d;HEX=$%s]', [Cardinal(Msg), IntToHex(Cardinal(Msg))]);
end;

